//fibonacci program
#include<iostream>
using namespace std;
class c1
{
public:
void fibonacci(int a,int b,int n)
{
int i,next;
cout<<a<<endl;
cout<<b<<endl;
for(i=1;i<=n-2;i++)
{
next=a+b;
cout<<next<<endl;
a=b;
b=next;
}
}
};
int main()
{
int a=0,b=1,n;
cout<<"eter any number:";
cin>>a>>b>>n;
c1 obj;
obj.fibonacci(a,b,n);
}

